{
    "workbench.sideBar.location": "right",
    "window.zoomLevel": 1,
    "editor.tabSize": 4,
    "editor.wordWrap": "on",
    "editor.renderWhitespace": "all",
    "editor.fontLigatures": true,
    "editor.fontFamily": "fira-code-nerd, FiraCode Nerd Font, Consolas, 'Courier New', monospace",
    "editor.renderControlCharacters": true,
    "editor.inlineSuggest.suppressSuggestions": true,
    "editor.inlayHints.enabled": "offUnlessPressed",
    "terminal.integrated.accessibleViewPreserveCursorPosition": true,
    "terminal.integrated.enableImages": true,
    "terminal.integrated.sendKeybindingsToShell": true,
    "terminal.integrated.defaultProfile.linux": "bash",
    "go.lintTool": "golangci-lint",
    "dotenv.enableAutocloaking": true,
    "vscode-neovim.useWSL": true,
    "vscode-neovim.compositeKeys": {
        "jj": {
            "command": "vscode-neovim.escape",
        },
        "jk": {
            "command": "vscode-neovim.escape",
        },
    },
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1,
    },
    "vscode-neovim.ctrlKeysForNormalMode": [
        "b",
        "e",
        "f",
        "h",
        "i",
        "j",
        "k",
        "l",
        "m",
        "o",
        "r",
        "t",
        "u",
        "v",
        "w",
        "x",
        "y",
        "z",
        "/",
        "]",
        "right",
        "left",
        "up",
        "down",
        "backspace",
        "delete"
    ],
    "vscode-neovim.ctrlKeysForInsertMode": [
        "h",
        "j",
        "m",
        "o",
        "r",
        "t",
        "u",
        "w"
    ],
    "vscode-neovim.revealCursorScrollLine": true
}